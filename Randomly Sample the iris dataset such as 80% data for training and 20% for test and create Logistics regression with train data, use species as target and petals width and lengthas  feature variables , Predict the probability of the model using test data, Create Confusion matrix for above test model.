library(dplyr)
library(caTools)
library(nnet)
data(iris)
set.seed(123)
iris$target <- as.factor(ifelse(iris$Species == "versicolor", 1, 0))
split <- sample.split(iris$target, SplitRatio = 0.8)
train_data <- subset(iris, split == TRUE)
test_data <- subset(iris, split == FALSE)
logreg_model <- glm(target ~ Petal.Width + Petal.Length, data = train_data, family = "binomial")
conf_matrix <- table(predicted_class, test_data$Species)
print(conf_matrix)
