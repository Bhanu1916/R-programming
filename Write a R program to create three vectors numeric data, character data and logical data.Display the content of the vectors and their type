numeric_vector <- c(1.5, 2.8, 3.0, 4.2, 5.1)
character_vector <- c("apple", "banana", "orange", "grape", "kiwi")
logical_vector <- c(TRUE, FALSE, TRUE, TRUE, FALSE)
cat("Numeric Vector:", numeric_vector, "\n")
cat("Character Vector:", character_vector, "\n")
cat("Logical Vector:", logical_vector, "\n")
cat("Data Type of Numeric Vector:", class(numeric_vector), "\n")
cat("Data Type of Character Vector:", class(character_vector), "\n")
cat("Data Type of Logical Vector:", class(logical_vector), "\n")
